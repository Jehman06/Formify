{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/forms/(.*)",
            "dest": "/forms/$1"
        },
        {
            "src": "/projects/(.*)",
            "dest": "/projects/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]    
}